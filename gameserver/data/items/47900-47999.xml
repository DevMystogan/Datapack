<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
    <weapon id="47927" name="Magic Fan - Blue" icon="icon.ev_kr_13th_present_blue">
        <!-- Within the swift breezes you can feel a magic energy. This magic fan is given out to commemorate the 13th anniversary of Lineage 2. -->
        <set name="weapon_type" val="BLUNT"/>
        <set name="bodypart" val="rhand"/>
        <set name="immediate_effect" val="true"/>
        <set name="soulshots" val="1"/>
        <set name="spiritshots" val="1"/>
        <set name="is_magic_weapon" val="true"/>
        <restriction tradable="false" dropable="false" depositable="false" sellable="false" freightable="true"/>
        <stats>
            <stat type="pAtk">1</stat>
            <stat type="mAtk">1</stat>
            <stat type="rCrit">6</stat>
            <stat type="accCombat">8</stat>
            <stat type="pAtkSpd">379</stat>
            <stat type="randomDamage">1</stat>
            <stat type="pAtkRange">40</stat>
        </stats>
    </weapon>
    <item id="47928" name="Magic Woodpile" icon="icon.ev_firewood_seed_solo">
        <!-- Double-click to use a magic woodpile for one player. If you have the Magic Fan equipped, you can use the skills Breeze and Gust to set fire to the woodpile. Will be deleted after the event. -->
        <set name="default_action" val="SKILL_REDUCE"/>
        <restriction stackable="true"/>
    </item>
    <item id="47929" name="Large Magic Woodpile" icon="icon.ev_firewood_seed_party">
        <!-- Double-click to use a magic woodpile for party use. If you have the Magic Fan equipped, you can use the skills Breeze and Gust to set fire to the woodpile. Will be deleted after the event. -->
        <set name="default_action" val="SKILL_REDUCE"/>
        <restriction stackable="true"/>
    </item>
    <weapon id="47945" name="Magic Fan - Yellow" icon="icon.ev_kr_13th_present_yellow">
        <!-- Within the swift breezes you can feel a magic energy. This magic fan is given out to commemorate the 13th anniversary of Lineage 2. -->
        <set name="weapon_type" val="BLUNT"/>
        <set name="bodypart" val="rhand"/>
        <set name="immediate_effect" val="true"/>
        <set name="soulshots" val="1"/>
        <set name="spiritshots" val="1"/>
        <set name="is_magic_weapon" val="true"/>
        <restriction tradable="false" dropable="false" depositable="false" sellable="false" freightable="true"/>
        <stats>
            <stat type="pAtk">1</stat>
            <stat type="mAtk">1</stat>
            <stat type="rCrit">6</stat>
            <stat type="accCombat">8</stat>
            <stat type="pAtkSpd">379</stat>
            <stat type="randomDamage">1</stat>
            <stat type="pAtkRange">40</stat>
        </stats>
    </weapon>
</list>
