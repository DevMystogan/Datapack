<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
	<weapon id="37700" name="Valentine Dual Dagger" type="DUAL_DAGGER" body-part="TWO_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a weapon to that of a Valentine Dual Dagger. -->
		<restriction freightable="false" tradable="false" dropable="false"/>
		<attributes commission-type="OTHER_ITEM" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<consume />
		<stats>
			<stat type="pAtk" value="752.0"/>
			<stat type="mAtk" value="414.0"/>
			<stat type="rCrit" value="12.0"/>
			<stat type="accCombat" value="-3.75"/>
			<stat type="pAtkSpd" value="433.0"/>
			<stat type="randomDamage" value="10.0"/>
			<stat type="pAtkRange" value="40.0"/>
		</stats>
	</weapon>

	<weapon id="37701" name="Valentine Warrior Dual Blunt Weapon" type="DUAL_BLUNT" body-part="TWO_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a weapon to that of a Valentine Dual Blunt Weapon. -->
		<restriction freightable="false" tradable="false" dropable="false"/>
		<attributes commission-type="OTHER_ITEM" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<consume />
		<stats>
			<stat type="pAtk" value="916.0"/>
			<stat type="mAtk" value="414.0"/>
			<stat type="rCrit" value="6.0"/>
			<stat type="accCombat" value="8.0"/>
			<stat type="pAtkSpd" value="325.0"/>
			<stat type="randomDamage" value="20.0"/>
			<stat type="pAtkRange" value="40.0"/>
		</stats>
	</weapon>

	<armor id="37702" name="Valentine Shield" type="SHIELD" body-part="LEFT_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a weapon to that of a Valentine Shield. -->
		<restriction freightable="false" tradable="false" dropable="false"/>
		<attributes enchant-enabled="false" />
		<crystal />
		<stats>
			<stat type="rEvas" value="-8.0"/>
			<stat type="rShld" value="20.0"/>
			<stat type="sDef" value="546.0"/>
		</stats>
	</armor>

	<armor id="37703" name="Valentine Sigil" type="NONE" body-part="LEFT_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a sigil to that of a Valentine Sigil. -->
		<restriction freightable="false" tradable="false" dropable="false"/>
		<attributes enchant-enabled="false" />
		<crystal />
		<stats>
			<stat type="pDef" value="150.0"/>
			<stat type="pAtkRange" value="40.0"/>
		</stats>
	</armor>

	<item id="37704" name="Love Potion" type="NONE" icon="icon.etc_whiteday_herb">
	<!-- Herb to change the appearance of a Valentine's Day weapon. -->
		<restriction freightable="false" sellable="false" depositable="false"/>
		<attributes for-npc="true" immediate-effect="true" ex-immediate-effect="true"/>
		<skill-reducer type="ItemSkills">
			<skill id="17154" level="1"/>  <!-- Elixir of Life -->
		</skill-reducer>
	</item>

	<item id="37705" name="Gold Fishing Shot" type="SOULSHOT" icon="icon.etc_spirit_bullet_fish_i05">
	<!-- Fishing consumables. Fishing success +100%. Cannot be sold in the grocery store. -->
		<restriction freightable="false" stackable="true" depositable="false"/>
		<attributes weight="3" price="56" immediate-effect="true" action="FISHINGSHOT"/>
	</item>

	<armor id="37725" name="NC Dinos Uniform" type="HEAVY" body-part="ALL_DRESS" icon="icon.london_olympic_bikini">
	<!-- NC Dinos Uniform. Cannot be exchanged, dropped, or sold. Can be stored in a private warehouse. -->
		<restriction freightable="false" tradable="false" dropable="false"/>
		<attributes weight="1000" enchant-enabled="false" />
		<crystal />
	</armor>

	<armor id="37726" name="NC Dinos Shortstop Cap" type="NONE" body-part="HAIR_ALL" icon="icon.accessory_mining_cap_i00">
	<!-- NC Dinos Defense Hat. -->
		<restriction freightable="false" destroyable="false" tradable="false" dropable="false"/>
		<attributes enchant-enabled="false" />
		<crystal />
	</armor>

</list>