<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
    <armor id="37533" name="NC Messenger's Hat" icon="icon.nc_hair_acc">
        <!-- NC Messenger's Hat. Occupies 2 Hair Accessory slots. STR +1, DEX + 1, INT + 1, WIT + 1. -->
        <set name="default_action" val="EQUIP"/>
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <restriction tradable="false" dropable="false" depositable="false" sellable="false" freightable="true"/>
    </armor>
    <armor id="37534" name="Santa Suit" icon="icon.armor_ev_santas_clothes">
        <!-- A costume to wear during Christmas holidays. -->
        <set name="default_action" val="EQUIP"/>
        <set name="armor_type" val="HEAVY"/>
        <set name="bodypart" val="alldress"/>
        <set name="immediate_effect" val="true"/>
        <restriction tradable="false" dropable="false" depositable="false" sellable="false"/>
    </armor>
    <armor id="37535" name="Santa Hat" icon="icon.accessory_santas_cap_i00">
        <!-- Santa Hat. -->
        <set name="default_action" val="EQUIP"/>
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <restriction tradable="false" dropable="false" depositable="false" sellable="false"/>
    </armor>
    <item id="37543" name="Snowy Squash Nectar" icon="icon.etc_nectar_i01">
        <!-- Item for Squash event 2015. Can be exchanged into Snowy Squash Seed or Large Snowy Squash Seed through Santa Claus to grow a Snowy Squash or a Large Snowy Squash. Will be deleted after event. -->
        <set name="default_action" val="SKILL_REDUCE"/>
        <set name="commissionItemType" val="OTHER_ITEM"/>
        <restriction sellable="false" stackable="true" olympiad-restricted="true"/>
        <skills>
            <skill id="17110" level="1"/> <!-- Snowy Squash Nectar -->
        </skills>
    </item>
    <item id="37544" name="Snowball" icon="BranchSys2.icon.g_illumination_white">
        <!-- Obtained from Squash. They say it brings good luck. Can be sold in any store or given to Aden Reconstruction Society. Will be deleted after event. -->
        <set name="price" val="5000"/>
        <restriction sellable="false" stackable="true"/>
    </item>
    <item id="37545" name="Large Snowball" icon="BranchSys2.icon.g_illumination_white">
        <!-- Obtained from Large Squash. They say it brings good luck. Can be sold in any store or given to Aden Reconstruction Society. Will be deleted after event. -->
        <set name="price" val="30000"/>
        <restriction sellable="false" stackable="true"/>
    </item>
    <item id="37546" name="Rudolf's Gift 2015" icon="icon.etc_pi_gift_box_i04">
        <!-- Santa's Reward. Inside there is Snowy Squash Nectar (10 pcs.). Will be removed after the event is over. -->
        <set name="default_action" val="PEEL"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction stackable="true"/>
    </item>
    <item id="37547" name="Rudolf's Gift 2015" icon="icon.etc_pi_gift_box_i04">
