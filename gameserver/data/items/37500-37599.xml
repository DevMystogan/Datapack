        <set name="weight" val="10"/>
        <restriction stackable="true"/>
    </item>
    <item id="37548" name="Rudolf's Gift 2015" icon="icon.etc_pi_gift_box_i04">
        <!-- Santa's Reward. Inside there is Snowy Squash Nectar (25 pcs.). Will be removed after the event is over. -->
        <set name="default_action" val="PEEL"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
