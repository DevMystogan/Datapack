    <item id="37549" name="Rudolf's Gift 2015" icon="icon.etc_pi_gift_box_i04">
        <!-- Santa's Reward. Inside there is Snowy Squash Nectar (60 pcs.). Will be removed after the event is over. -->
        <set name="default_action" val="PEEL"/>
