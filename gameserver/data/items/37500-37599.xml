    <item id="37550" name="Snowman's Buff" icon="icon.weapon_snowman_stick_i00">
        <!-- When in a peaceful zone, restores energy for 3 hours. Will be deleted after event. -->
        <set name="default_action" val="SKILL_REDUCE"/>
        <set name="weight" val="120"/>
        <restriction stackable="true"/>
    </item>
    <item id="37551" name="Christmas Present" icon="icon.etc_pi_gift_box_i04">
        <!-- Double-click to obtain one of the following presents: Bottle of Orfen's Soul, Bottle of Frintezza's Soul, Bottle of Queen Ant's Soul, Bottle of Baium's Soul. Will be deleted after the event is over. -->
        <set name="default_action" val="CAPSULE"/>
        <set name="immediate_effect" val="true"/>
        <restriction sellable="false" stackable="true"/>
    </item>
    <item id="37552" name="Snowy Squash Seed" icon="icon.etc_gourd_i10">
