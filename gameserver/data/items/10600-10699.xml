<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
    <item id="10648" name="Inventory2" icon="icon.blessed_feather_i00">
        <!-- Doesn't exist in client. -->
        <set name="immediate_effect" val="true"/>
        <set name="ex_immediate_effect" val="true"/>
        <restriction tradable="false" dropable="false" sellable="false" depositable="false" clan-depositable="false" mailable="false" commissionable="false" stackable="true"/>
    </item>
    <item id="10649" name="Feather of Blessing" icon="">
        <!-- Dimensional item.  Blessed feather that allows to resurrect right in the place of death. -->
        <set name="allow_self_resurrection" val="true"/>
        <restriction sellable="false" stackable="true" clan-depositable="false" mailable="false" commissionable="false" freightable="true"/>
    </item>
    <item id="10650" name="Newbie Scroll of Escape" icon="icon.etc_scroll_of_return_i00">
        <!-- Magic scroll that moves you to the nearest village. Item for beginner adventurers. Cannot be exchanged, dropped, or sold. Can be stored in a private warehouse. -->
        <set name="default_action" val="SKILL_REDUCE"/>
        <set name="etcitem_type" val="SCROLL"/>
        <set name="weight" val="120"/>
        <set name="handler" val="ItemSkills"/>
        <restriction tradable="false" dropable="false" sellable="false" stackable="true" olympiad-restricted="true" clan-depositable="false" mailable="false" commissionable="false"/>
        <skills>
            <skill id="2531" level="1"/> <!-- Adventurer's Scroll of Escape -->
        </skills>
    </item>
</list>
