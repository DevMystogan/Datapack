<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
    <armor id="20020" name="Uniform Hat" icon="BranchSys.icon.br_Uniform_Hat_i00">
        <!-- A very fancy hat. Occupies 2 Hair Accessory slots. -->
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction sellable="false"/>
    </armor>
    <armor id="20021" name="Assassin's Bamboo Hat" icon="BranchSys.icon.br_Bamboo_Hat_i00">
        <!-- A hat made from bulrush. Occupies 2 Hair Accessory slots. -->
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction sellable="false"/>
    </armor>
    <armor id="20022" name="Ruthless Tribe Mask" icon="BranchSys.icon.br_mask_i00">
        <!-- A mask which formerly belonged to Looters. -->
        <set name="bodypart" val="hair"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction sellable="false"/>
    </armor>
    <armor id="20023" name="Ribbon Hairband" icon="BranchSys.icon.br_Ribbon_i00">
        <!-- A ribbon-like hair accessory, intended for female characters only. -->
        <set name="bodypart" val="hair"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction sellable="false"/>
    </armor>
    <item id="20033" name="Unoccupied Teleport Device Flag" icon="BranchSys.icon.br_flag_of_bookmark_i00">
        <!-- The item is used when the current location of an Unoccupied Teleport Device is saved. -->
        <set name="weight" val="5"/>
        <restriction stackable="true"/>
    </item>
</list>
