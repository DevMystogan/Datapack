<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
    <armor id="26326" name="Lineage II Baseball Uniform" icon="icon.london_olympic_bikini">
        <!-- Lineage II baseball uniform. When equipped, armor is removed and cannot be used. Hair accessories, weapons, and magic armor are not removed. -->
        <set name="armor_type" val="HEAVY"/>
        <set name="bodypart" val="alldress"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="1000"/>
        <restriction sellable="false"/>
    </armor>
    <armor id="26327" name="Lineage II Baseball Cap" icon="icon.accessory_mining_cap_i00">
        <!-- Lineage II baseball cap. -->
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction sellable="false"/>
    </armor>
    <item id="26354" name="Gem" icon="icon.etc_talisman_energy_i01">
        <!-- Used for purchase in Game Store. -->
        <restriction tradable="false" dropable="false" depositable="false" sellable="false" stackable="true"/>
    </item>
    <armor id="26356" name="Pirate Outfit" icon="icon.bm2018_pirate_i01">
        <!-- Pirate outfit. -->
        <set name="armor_type" val="HEAVY"/>
        <set name="bodypart" val="alldress"/>
        <set name="immediate_effect" val="true"/>
        <restriction sellable="false"/>
    </armor>
</list>
