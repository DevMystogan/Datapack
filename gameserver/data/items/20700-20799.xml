<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
    <item id="20728" name="Summon Scroll: Lucky Lamp" icon="BranchSys.icon.br_lamp_orange_i00">
        <!-- Summon Scroll: Lucky Lamp allows to resurrect at the place of death. You are invisible for the first 5 sec after resurrection. Monsters' attacks have no effect for 2 min. Movement speed is reduced by 50% for the duration of the effect. -->
        <set name="default_action" val="SKILL_REDUCE"/>
        <restriction tradable="false" dropable="false" depositable="false" sellable="false" stackable="true"/>
    </item>
    <armor id="20792" name="Flavorful Cheese Hat" icon="BranchSys.icon.br_bigrat_cap_i00">
        <!-- A hat shaped like a mouth-watering piece of cheese. On closer examination, you can see an orange mouse on top of it. -->
        <set name="default_action" val="EQUIP"/>
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction dropable="false" sellable="false"/>
    </armor>
    <armor id="20793" name="Sweet Cheese Hat" icon="BranchSys.icon.br_goldrat_cap_i00">
        <!-- A hat shaped like a mouth-watering piece of cheese. On closer examination, you can see a purple mouse on top of it. -->
        <set name="default_action" val="EQUIP"/>
        <set name="bodypart" val="hairall"/>
        <set name="immediate_effect" val="true"/>
        <set name="weight" val="10"/>
        <restriction dropable="false" sellable="false"/>
    </armor>
</list>
