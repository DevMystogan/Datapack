	<item id="45481" name="Fresh Blue Mackerel" type="NONE" icon="icon.fish_normal_i12">
	<!-- Trade for various items through a fishing guild member. -->
		<restriction freightable="false" stackable="true" depositable="false"/>
		<attributes/>
	</item>

	<item id="45488" name="Golden Treasure Chest" type="NONE" icon="icon.etc_treasure_box_i08">
	<!-- Double-click to get an item. -->
		<restriction freightable="false" stackable="true" depositable="false"/>
		<attributes weight="10"/>
	</item>

	<weapon id="45492" name="Fishing Rod" type="FISHING_ROD" body-part="TWO_HAND" icon="icon.weapon_fishing_rod_i03">
	<!-- A Fishing Rod for one day.  Consumes 1 Fishing Shot. -->
		<restriction freightable="false" tradable="false" dropable="false"/>
		<attributes weight="1000" commission-type="OTHER_ITEM" duration="1440" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<consume />
		<stats>
			<stat type="pAtk" value="1.0"/>
			<stat type="mAtk" value="1.0"/>
			<stat type="accCombat" value="-3.75"/>
			<stat type="pAtkSpd" value="325.0"/>
			<stat type="randomDamage" value="1.0"/>
			<stat type="pAtkRange" value="80.0"/>
		</stats>
	</weapon>

	<item id="45493" name="Fishing Rod Chest (1-day)" type="NONE" icon="icon.etc_box_of_adventure_1_i00">
	<!-- Use it to obtain a 1-day Fishing Rod.  -->
		<restriction freightable="false" stackable="true" depositable="false"/>
		<attributes weight="100" price="20514" immediate-effect="true"/>
		<extract >
			<item id="45492" min-count="1" max-count="1" chance="100000"/>  <!-- Fishing Rod -->
		</extract>
	</item>

	<item id="45495" name="Normal Bait" type="LURE" icon="icon.etc_huge_lure">
	<!-- Bait for fishing. Use to catch fish.  -->
		<restriction freightable="false" stackable="true" depositable="false"/>
		<attributes price="7" immediate-effect="true"/>
	</item>

</list>